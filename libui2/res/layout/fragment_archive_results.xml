                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/network_operator_code"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_network_operator_code"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_network_operator_code"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_roaming_status"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/roaming_status"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_roaming_status"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_roaming_status"/>

                </LinearLayout>

                <TextView
                    android:id="@+id/fragment_archived_results_passive_metric_label_signal"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/visual_rhythm_half"
                    android:gravity="bottom"
                    android:text="@string/signal"
                    android:textColor="@color/white_alpha_0.5"
                    android:textSize="@dimen/text_size_small"/>

                <LinearLayout
                    android:id="@+id/fragment_archived_results_passive_metric_divider_signal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divider_height"
                    android:background="@color/white_alpha_0.5"
                    android:orientation="vertical" >
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_cell_tower_ID"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/cell_tower_id"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_cell_tower_id"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_cell_tower_id"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_cell_tower_area_location_code"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/cell_tower_area_location_code"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_cell_tower_area_location_code"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_cell_tower_area_location_code"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_signal_strength"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/signal_strength"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_signal_strength"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="-89 dBm"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_bearer"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/bearer"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_bearer"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_bearer"/>

                </LinearLayout>

                <TextView
                    android:id="@+id/fragment_archived_results_passive_metric_label_device"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/visual_rhythm_half"
                    android:gravity="bottom"
                    android:text="@string/device"
                    android:textColor="@color/white_alpha_0.5"
                    android:textSize="@dimen/text_size_small"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divider_height"
                    android:background="@color/white_alpha_0.5">
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_manufacturer"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/manufacturer"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_manufacturer"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_manufacturer"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_model"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/model"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_detail_model"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="Nexus 5"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_OS"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/os"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_detail_OS"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_os"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_OS_vesion"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/os_version"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_OS_version"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="19"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_phone_type"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/phone_type"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_phone_type"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_phone_type"/>

                </LinearLayout>

                <TextView
                    android:id="@+id/fragment_archived_results_passive_metric_label_location"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/visual_rhythm_half"
                    android:gravity="bottom"
                    android:text="@string/location"
                    android:textColor="@color/white_alpha_0.5"
                    android:textSize="@dimen/text_size_small"/>

                <LinearLayout
                    android:id="@+id/fragment_archived_results_passive_metric_divider_location"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divider_height"
                    android:background="@color/white_alpha_0.5"
                    android:orientation="vertical">
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_latitude"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/latitude"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_latitude"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_latitude"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_longitude"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/longitude"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_longitude"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_longitude"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_accuracy"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/accuracy"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_accuracy"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_accuracy"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/spacing_small">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_location_provider"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
