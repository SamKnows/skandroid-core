<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_archived_results_rl_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:background="@drawable/background_gradient_radial_blue_vivid">

    <LinearLayout
        android:id="@+id/fragment_archived_results_dummy_row"
        android:layout_width="match_parent"
        android:layout_height="@dimen/fragment_run_test_results_layout_height"
        android:layout_marginBottom="@dimen/spacing_small"
        android:layout_marginLeft="@dimen/spacing_small"
        android:layout_marginRight="@dimen/spacing_small"
        android:layout_marginTop="@dimen/spacing_small"
        android:background="@drawable/selector_summary_background_black_alpha"
        android:baselineAligned="false"
        android:gravity="center"
        android:orientation="horizontal"
        android:visibility="gone"
        tools:visibility="visible">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:paddingLeft="@dimen/spacing_small">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/spacing_medium"
                    android:layout_height="@dimen/spacing_medium"
                    android:alpha=".75"
                    android:contentDescription="@string/description_icon_download"
                    android:src="@drawable/ic_download" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/download"
                    android:textColor="@color/blue"
                    android:textSize="@dimen/text_size_small" />

            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/spacing_medium_negative"
                android:layout_marginTop="@dimen/spacing_medium_negative"
                android:text="@string/nineninenine"
                android:textColor="@android:color/white"
                android:textSize="@dimen/text_size_peta" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/units_Mbps"
                android:textColor="@android:color/white"
                android:textSize="@dimen/text_size_small"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:gravity="center_vertical"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/spacing_medium"
                    android:layout_height="@dimen/spacing_medium"
                    android:alpha=".75"
                    android:contentDescription="@string/description_icon_upload"
                    android:src="@drawable/ic_upload" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/upload"
                    android:textColor="@color/blue"
                    android:textSize="@dimen/text_size_small" />

            </LinearLayout>

            <com.samknows.measurement.activity.components.FontFitTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/spacing_medium_negative"
                android:layout_marginTop="@dimen/spacing_medium_negative"
                android:text="@string/nineninenine"
                android:textColor="@color/grey_light"
                android:textSize="@dimen/text_size_peta"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/units_Mbps"
                android:textColor="@android:color/white"
                android:textSize="@dimen/text_size_small"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:text="@string/latency"
                    android:textColor="@color/blue"
                    android:textSize="@dimen/text_size_small" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:text="@string/loss"
                    android:textColor="@color/blue"
                    android:textSize="@dimen/text_size_small" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:text="@string/jitter"
                    android:textColor="@color/blue"
                    android:textSize="@dimen/text_size_small" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <com.samknows.measurement.activity.components.FontFitTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/nineninenine"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/text_size_small_medium" />

                <com.samknows.measurement.activity.components.FontFitTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/nineninenine"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/text_size_small_medium" />

                <com.samknows.measurement.activity.components.FontFitTextView
                    android:id="@+id/archiveResultsListItemJitter"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/nineninenine"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/text_size_small_medium" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/fragment_archived_results_warning_no_results_yet"
        android:layout_width="match_parent"
        android:layout_height="@dimen/visual_rhythm"
        android:layout_alignParentBottom="true"
        android:background="@color/orange"
        android:clickable="true"
        android:gravity="center"
        android:padding="@dimen/spacing_small"
        android:text="@string/no_archived_results_yet"
        android:textColor="@color/black"
        android:textSize="@dimen/text_size_medium"
        android:visibility="gone"
        tools:visibility="visible">
    </TextView>

    <LinearLayout
        android:id="@+id/fragment_archived_results_ll_passive_metrics"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/spacing_small"
        android:layout_marginLeft="@dimen/spacing_small"
        android:layout_marginRight="@dimen/spacing_small"
        android:layout_marginTop="136dp"
        android:background="@drawable/background_black_alpha"
        android:orientation="vertical"
        tools:visibility="visible">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:visibility="gone" >

            <LinearLayout
                android:id="@+id/fragment_archived_results_ll_inner_passive_metrics"
