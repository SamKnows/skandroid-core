                    android:layout_height="@dimen/visual_rhythm_half"
                    android:gravity="bottom"
                    android:text="@string/sim_and_network_operators"
                    android:textColor="@color/white_alpha_0.5"
                    android:textSize="@dimen/text_size_small"/>

                <LinearLayout
                    android:id="@+id/fragment_archived_results_passive_metric_divider_sim_and_network_operators"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divider_height"
                    android:background="@color/white_alpha_0.5"
                    android:orientation="vertical">
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_sim_operator"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/sim_operator"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_sim_operator_name"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_sim_operator"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_sim_operator_code"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/sim_operator_code"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"/>

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_result_sim_operator_code"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="@color/orange"
                        android:textSize="@dimen/text_size_small_medium"
                        tools:text="@string/tools_text_value_sim_operator_code"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/fragment_archived_results_passive_metric_label_network_operator"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/visual_rhythm_half"
                        android:layout_weight="1"
